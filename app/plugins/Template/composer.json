{
    "name": "kmp/template",
    "description": "Template plugin for KMP - A complete boilerplate for creating new plugins",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Template\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Template\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}