{
    "name": "kmp/waivers",
    "description": "Waivers plugin for KMP - Gathering Waiver Tracking System",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Waivers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waivers\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}